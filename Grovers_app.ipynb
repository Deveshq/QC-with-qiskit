   "source": [
    "# Defining a function which flip the sign of the marked state, in the superpositon of all the states.\n",
    "\n",
    "def orc(d):\n",
    "    op = np.identity(2**n)\n",
    "    for i in d:\n",
    "        op[i][i] = op[i][i]*(-1)\n",
    "        display(i)\n",
    "    return op"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "d43831d8-6cc5-4c34-93bf-0740040d6b64",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Defining another function which flip all the states about the average.\n",
    "\n",
    "def dif():\n",
    "    op = np.identity(2**n)\n",
    "    for i in range(2**n):\n",
    "        if ( i != 0):\n",
    "            op[i][i] = op[i][i]*(-1)\n",
    "    #display(op)\n",
    "    \n",
    "    return op"
   ]
  },
  {
