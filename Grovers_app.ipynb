   "source": [
    "# Taking vector of integer number as input.\n",
    "display(Latex(r'Expected a vector of length $2^n$ as input!'))\n",
    "\n",
    "n = int(input('Enter n: '))\n",
    "a = []\n",
    "print('Enter the vector:')\n",
    "for i in range(2**n):\n",
    "    a.append(int(input()))\n",
    "\n",
    "print(f'a = {a}')\n"
   ]
  },
  {
