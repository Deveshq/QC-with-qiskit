    }
   ],
   "source": [
    "#Finally applying all the component on the circuit.\n",
    "for i in range(t):\n",
    "    qc.unitary(orc(d), range(n))\n",
    "    qc.h(range(n))\n",
    "    qc.unitary(dif(), range(n))\n",
    "    qc.h(range(n))\n",
    "\n",
    "qc.draw()"
   ]
